/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "portfolio",
  "main": ".svelte-kit/cloudflare/_worker.js",
  "compatibility_date": "2025-12-23",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": ".svelte-kit/cloudflare",
  },
  "preview_urls": false,
  "observability": {
    "enabled": false,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
    }
  },
  "ratelimits": [
    {
      "name": "IMG_RATE_LIMITER",
      "namespace_id": "1001",
      "simple": {
        "limit": 10,
        "period": 60
      }
    }
  ],
  "kv_namespaces": [
    {
      "binding": "UNSPLASH_CACHE",
      "id": "03de99aafdc148e982f219a2c5d3cf68",
      "remote": false
    }
  ],
  "route": {
    "custom_domain": true,
    "pattern": "thelukez.com"
  }
}